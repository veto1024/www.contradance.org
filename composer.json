{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "ext-intl": "*",
        "ext-json": "*",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php": "^3.112",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address_map_link": "^1.0",
        "drupal/admin_toolbar": "^2.0",
        "drupal/background_image": "^1.3",
        "drupal/background_video": "1.x-dev",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/bootstrap": "^3.21",
        "drupal/bootstrap_toggle": "^1.2",
        "drupal/coffee": "^1.0",
        "drupal/commerce": "^2.14",
        "drupal/commerce_choose_price": "^1.2",
        "drupal/commerce_pado": "^1.0@RC",
        "drupal/commerce_reports": "^1.0",
        "drupal/commerce_stripe": "^1.0",
        "drupal/commerce_webform_order": "1.x-dev",
        "drupal/computed_field": "^2.0",
        "drupal/conditional_fields": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/courier": "^1.0@alpha",
        "drupal/date_pager": "1.x-dev",
        "drupal/devel": "^2.1",
        "drupal/entity_browser": "^2.2",
        "drupal/entity_print": "^2.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/feeds": "^3.0",
        "drupal/field_group": "^3.0",
        "drupal/flag": "^4.0",
        "drupal/flag_search_api": "^1.4",
        "drupal/fullcalendar_view": "^2.4",
        "drupal/google_analytics": "^2.4",
        "drupal/link_attributes": "^1.9",
        "drupal/mailchimp": "^1.11",
        "drupal/mailchimp_ecommerce": "^1.5",
        "drupal/mailsystem": "^4.1",
        "drupal/mandrill": "^1.1",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.10",
        "drupal/officialfacebookpixel": "^1.1",
        "drupal/paragraphs": "^1.9",
        "drupal/paragraphs_edit": "^2.0",
        "drupal/reroute_email": "^1.1",
        "drupal/riddler": "^1.1",
        "drupal/rules": "^3.0",
        "drupal/rules_webform": "^1.4",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/s3fs_cors": "^1.0@beta",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_db": "^1.15",
        "drupal/sendgrid_integration": "^1.2",
        "drupal/slick_extras": "^1.0",
        "drupal/social_login": "^2.89",
        "drupal/swiftmailer": "^1.0",
        "drupal/telephone_validation": "^2.2",
        "drupal/token": "^1.5",
        "drupal/veoa": "^1.0",
        "drupal/videobackground": "^1.2",
        "drupal/views_advanced_routing": "^1.0",
        "drupal/views_aggregator": "^1.0",
        "drupal/views_bootstrap": "^3.1",
        "drupal/views_data_export": "^1.0",
        "drupal/webform_encrypt": "^1.0",
        "drupal/webform_views": "^5.0",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrushLinker\\Linker::linkDrush"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
